{
  "manifest_version": 2,
  "name": "youtube_player helper",
  "version": "0.3",
  "description": "Does some helpful stuff to help with https://thenamlessguy.github.io/youtube_player/",
  "homepage_url": "https://github.com/TheNamlessGuy/youtube_player_helper",
  "icons": {
    "48": "icons/ico.png"
  },

  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab"
  ],

  "page_action": {
    "browser_style": true,
    "default_icon": "icons/ico.png",
    "default_title": "youtube_player helper"
  },

  "background": {
    "scripts": [
      "helpers.js",

      "popups/youtube/background.js",

      "background.js"
    ]
  }
}
